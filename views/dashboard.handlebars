{{#if review.length}}
  <section>
<div class="">
  {{#each review}}
  {{ this.user.username }}'S DASHBOARD
  {{/each}}
</div>

    <h2 class='review-header'> Reviews </h2>

    {{#each review as |i|}}
      <div class='reviews-container'>
        <div class='reviews'>
          {{this.reviews_text}} ---- this review was made on {{format_date createdAt}}

          <a href='/dashboard/edit/{{user_id}}' class='edit-link'>Edit</a>
        </div>
      </div>

    {{/each}}

  </section>

{{/if}}